{
  "name": "api-blogs",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "debug": "nest start --debug --watch",
    "start": "cross-env NODE_ENV=production node dist/main",
    "pm2": "pm2 start pnpm --no-pmx --name api-blogs -- start",
    "lint": "eslint --cache --ext .js,.ts, --format=pretty ./src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "prettier": "prettier -c --write \"src/**/*\"",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx, --format=pretty ./src"
  },
  "lint-staged": {
    "**/*.{js,ts}": "npm run lint-staged:js",
    "**/*.{js,ts,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.465.0",
    "@nestjs/common": "^9.0.5",
    "@nestjs/core": "^9.0.5",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/schedule": "^2.2.3",
    "@nestjs/swagger": "^7.1.8",
    "axios": "^1.6.5",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.14.0",
    "co": "^4.6.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.9",
    "dotenv": "^8.2.0",
    "js-base64": "^3.7.5",
    "js-yaml": "^4.1.0",
    "mime-types": "^2.1.35",
    "mongoose": "^5.13.16",
    "multer": "1.4.5-lts.1",
    "nestjs-typegoose": "^7.1.38",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.4",
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.7",
    "@typegoose/typegoose": "^7.4.8",
    "@types/cron": "^1.7.2",
    "@types/crypto-js": "^4.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^14.14.22",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "commitlint": "^12.1.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "lint-staged": "^12.5.0",
    "prettier": "^2.5.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.9.5"
  }
}
