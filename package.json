{
  "name": "api-blogs",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "debug": "nest start --debug --watch",
    "start": "cross-env NODE_ENV=production node dist/main",
    "pm2": "pm2 start npm --no-pmx --name api-blogs -- start",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "prettier": "prettier -c --write \"src/**/*\"",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx "
  },
  "lint-staged": {
    "**/*.{js,ts}": "npm run lint-staged:js",
    "**/*.{js,ts,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@nestjs/common": "^7.6.7",
    "@nestjs/core": "^7.6.7",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.7",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/swagger": "^4.7.12",
    "axios": "^0.26.1",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "co": "^4.6.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "js-yaml": "^4.1.0",
    "mongoose": "^5.11.14",
    "nestjs-typegoose": "^7.1.38",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "qiniu": "^7.3.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.7",
    "@typegoose/typegoose": "^7.4.8",
    "@types/cron": "^1.7.2",
    "@types/crypto-js": "^4.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^14.14.22",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
